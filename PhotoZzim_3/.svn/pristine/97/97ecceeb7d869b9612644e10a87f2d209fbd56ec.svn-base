.photo-container{padding:20px;min-height:700px;margin:0}.photo-container .thumbnails{margin:0 auto}.photo-container .thumbnails .ui-selected.thumbnail{background:-webkit-linear-gradient(top,#a90329 0,#8f0222 44%,#6d0019 100%);color:white}.photo-container .thumbnails .thumbnail.friends{box-shadow:1px 2px 8px #f00}.photo-container .thumbnail{background:white;max-width:110px;-webkit-transition-duration:.7s;overflow:hidden}.photo-container .thumbnail img{border-radius:5px}.photo-container .thumbnail p{text-align:center;font-size:15px;margin:auto}@media(min-width:1200px){ul.thumbnails>li.span2{margin-left:5px;width:130px}}